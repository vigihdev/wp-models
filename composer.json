{
    "name": "vigihdev/wp-models",
    "description": "WordPress Models & Entities - Modern object-oriented models for WordPress development with type safety and IDE support",
    "keywords": [
        "wordpress",
        "models",
        "entities",
        "orm",
        "database",
        "wp-models",
        "object-oriented",
        "type-safe",
        "php8",
        "wordpress-development"
    ],
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "homepage": "https://github.com/vigihdev/wp-models",
    "authors": [
        {
            "name": "Vigih Dev",
            "email": "vigihdev@gmail.com",
            "role": "developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "10.5.x-dev"
    },
    "autoload": {
        "psr-4": {
            "Vigihdev\\WpModels\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vigihdev\\WpModels\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0.x-dev"
        }
    }
}